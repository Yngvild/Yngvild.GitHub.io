@article{Ruel1,
	Abstract = {Many biologists now recognize that environmental variance can exert
	important effects on patterns and processes in nature that are independent
	of average conditions. Jensens inequality is a mathematical proof
	that is seldom mentioned in the ecological literature but which provides
