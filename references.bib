	Date-Modified = {2010-09-28 14:06:34 +0200},
	Journal = {J. Anim. Ecol.},
	Pages = {722-740},
	Title = {Maternal and birth colony effects on survival of {W}eddell seal offspring from {M}c{M}urdo {S}ound, {A}ntarctica},
	Volume = {67},
	Year = {1998},
	Bdsk-File-1 = {},
